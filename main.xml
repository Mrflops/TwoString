<mujoco model="spirob">
    <!-- set some defaults for units and lighting -->
    <compiler angle="radian" meshdir="C:\Users\secre\Documents\FRC\TwoString\twoStringSTL"/>

    <!-- import our stl files -->
    <asset>
        <mesh file="TwoString19.STL" />
        <mesh file="TwoString18.STL" />
    </asset>

    <!-- define our robot model -->
    <worldbody>
        <!-- set up a light pointing down on the robot -->
        <light directional="true" pos="-0.5 0.5 3" dir="0 0 -1" />
        <!-- add a floor so we don't stare off into the abyss -->
        <geom name="floor" pos="0 0 0" size="1 1 1" type="plane" rgba="1 0.83 0.61 0.5"/>

        <!-- start building our model -->
        <body name="base" pos="0 0 0.1">
            <geom name="segment19" type="mesh" mesh="part19" pos="0 0 0" contype="1" conaffinity="1"/>
            <inertial pos="0 0 0" mass="0" diaginertia="0 0 0"/>
            <site name="cable1_anchor19" pos="0.0668 0.116 0" size="0.01" rgba="1 0 0 1" />
            <site name="cable2_anchor19" pos="0.0668 -0.116 0" size="0.01" rgba="1 0 0 1" />

            <!-- nest each child piece inside the parent body tags -->
            <body name="segment18" pos="0 0 0.135">
                <!-- this joint connects link1 to the base -->
                <joint name="joint18" type="ball" axis="1 0 0" pos="0 0 -0.109767" stiffness="150" damping="75"/>

                <geom name="segment18" type="mesh" mesh="part18" pos="0 0 0" euler="0 3.14 0" contype="1" conaffinity="1"/>
                <inertial pos="0 0 0" mass="0.5" diaginertia="1 1 1"/>
                <site name="cable1_anchor18" pos="0.06139 0.107 0" size="0.01" rgba="1 0 0 1" />
                <site name="cable2_anchor18" pos="0.06139 -0.107 0" size="0.01" rgba="1 0 0 1" />

                <!-- Additional nested bodies can be added here -->
            </body>
        </body>
    </worldbody>

    <!-- attach actuators to joints -->
    <actuator>
        <motor name="cable_motor1" tendon="cable1" gear="750"/>
        <motor name="cable_motor2" tendon="cable2" gear="750"/>
    </actuator>
</mujoco>
